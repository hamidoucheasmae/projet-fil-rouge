</head>

<body style="margin-bottom: 60px;">
  <!-- Navigation-->

  <div style="margin-left: 1750px; margin-top: 15px;">
  	<button onclick="window.print()" type="button" class="btn btn-outline-dark">Print</button>

  </div>
  
  <div id="print" class="content-wrapper">
    <div class="container-fluid">
      <!-- Breadcrumbs-->
     
      <!-- Example DataTables Card-->
      <div class="agile-header">
              <h1>PATIENT REPORT</h1>
    
        </div>

        <div>
          
          <form style="width: 75%; margin-left: 80px; font-weight: bold;">
  <div class="form-group row">
    <label for="inputEmail3" class="col-sm-2 col-form-label">Patient ID :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].id %>" type="text" class="form-control" id="inputEmail3" placeholder="Patient ID" name = "pid" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Patient Name :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].name %>" type="text" class="form-control" id="inputPassword3" placeholder="Patint Name" name="pname" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Date of Appointment :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].doa.getDate() %> - <%= list[0].doa.getMonth() + 1 %> - <%=list[0].doa.getFullYear()%>" type="text" class="form-control" id="inputPassword3" placeholder="Date Of Appointment" name="doa" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Doctor ID :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].d_id %>" type="text" class="form-control" id="inputPassword3" placeholder="Doctor ID" name="did" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Doctor Name :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].d_name %>" type="text" class="form-control" id="inputPassword3" placeholder="Doctor Name" name="dname" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Room :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].room %>" type="text" class="form-control" id="room" placeholder="Room Number" name="room" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Room Type :</label>
    <div class="col-sm-10">
      <input value="TO BE ADDED" type="text" class="form-control" id="rtype" placeholder="Room Type" name="rtype" disabled>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Medicines :</label>
    <div class="col-sm-10">
      <textarea value="<%= list[0].medicines %>" class="form-control" id="exampleFormControlTextarea1" rows="10" placeholder="Medicines" name="med" disabled><%= list[0].medicines %></textarea>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Description :</label>
    <div class="col-sm-10">
      <textarea value="<%= list[0].description %>" class="form-control" id="exampleFormControlTextarea1" rows="10" placeholder="Description" name="desc" disabled><%= list[0].description %></textarea>
    </div>
  </div>
  
</form>

        </div>
  <!--//header-->
  <!--main-->

  <hr>


     <div class="agile-header">
              <p style="font-size: 30px; font-weight: bold;">OTHER DETAILS<p>
    
        </div>

        <div>
          
          <form style="width: 75%; margin-left: 80px; font-weight: bold;">
  
  
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Address :</label>
    <div class="col-sm-10">
      <textarea class="form-control" id="address" rows="3" placeholder="Address" name="address" disabled><%= list[0].address %></textarea>
    </div>
  </div>
  
  <div class="form-group row">
    <label for="inputEmail3" class="col-sm-2 col-form-label">City :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].city %>" type="text" class="form-control" id="inputEmail3" placeholder="City" name = "city" disabled>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputEmail3" class="col-sm-2 col-form-label">State :</label>
    <div class="col-sm-10">
      <input value="<%= list[0].state %>" type="text" class="form-control" id="inputEmail3" placeholder="State" name = "state" disabled>
    </div>
  </div>

  <script type="text/javascript">
    var rid = document.getElementById("room").value;
    if(rid > 100 && rid <= 200){
      document.getElementById("rtype").value = 'GENERAL WARD';
    }
    if(rid > 200 && rid <= 300){
      document.getElementById("rtype").value = 'PERSONAL ROOM';
    }
    if(rid > 300 && rid <= 325){
      document.getElementById("rtype").value = 'ICU';
    }
  </script>
  
</form>

        </div>


      
    </div>

    
    <!-- /.container-fluid-->
    <!-- /.content-wrapper-->
    <footer class="sticky-footer">
      <div class="container">
        <div class="text-center">
          <small>Copyright Â© Your Website 2018</small>
        </div>
      </div>
    </footer>
    <!-- Scroll to Top Button-->
    
    <!-- Logout Modal-->

    <!-- Bootstrap core JavaScript-->
    <script src="/vendor/jquery/jquery.min.js"></script>
    <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Page level plugin JavaScript-->
    <script src="/vendor/datatables/jquery.dataTables.js"></script>
    <script src="/vendor/datatables/dataTables.bootstrap4.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="/js/sb-admin.min.js"></script>
    <!-- Custom scripts for this page-->
    <script src="/js/sb-admin-datatables.min.js"></script>
    <script src="/js/dropdown_script.js"></script>
  </div>
</body>

</html>
